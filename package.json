{
  "name": "nest-starter",
  "private": true,
  "version": "1.0.0",
  "description": "Nest.js starter",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"**/*.ts\"",
    "generate-schema": "nest start --entryFile generate-schema",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.cjs --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage --detectOpenHandles --forceExit",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.9.4",
    "@nestjs/apollo": "^12.0.9",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/graphql": "^12.0.9",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.7",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.1.1",
    "@nestjs/websockets": "^10.2.7",
    "axios": "^1.5.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "graphql-query-complexity": "^0.12.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.14.1",
    "helmet": "^7.0.0",
    "js-yaml": "^4.1.0",
    "nestjs-i18n": "^10.3.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.94",
    "@swc/jest": "^0.2.29",
    "@types/body-parser": "^1.19.4",
    "@types/cookie-parser": "^1.4.5",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.9",
    "@types/jest": "^29.5.6",
    "@types/js-yaml": "^4.0.8",
    "@types/node": "^18.18.6",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/supertest": "^2.0.15",
    "@types/webpack-env": "^1.18.3",
    "@types/webpack-node-externals": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "run-script-webpack-plugin": "^0.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-node-externals": "^3.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "@swc/jest"
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
